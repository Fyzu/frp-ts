{
  "version": 2,
  "projects": {
    "core": "packages/core",
    "fp-ts": "packages/fp-ts",
    "lens": "packages/lens",
    "react": "packages/react",
    "test-utils": "packages/test-utils",
    "utils": "packages/utils",
    "workspace": {
      "root": ".",
      "targets": {
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true,
            "commitMessageFormat": "${version}",
            "push": false
          }
        }
      }
    }
  }
}
